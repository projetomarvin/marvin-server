{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "activity": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "findById": true
        }
      }
    }
  },
  "course": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "findById": true,
          "create": true,
          "prototype.__create__students": true,
          "prototype.__create__activities": true
        }
      }
    }
  },
  "student": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "reset": true,
          "reset-password": true,
          "findById": true,
          "getUsername": true,
          "pushToGit": true,
          "checkRepository": true,
          "prototype.patchAttributes": true
        }
      }
    }
  },
  "studentActivity": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "checkFiles": true,
          "finish": true,
          "findById": true,
          "prototype.patchAttributes": true,
          "cancelCorrection": true,
          "redo": true
        }
      }
    }
  },
  "correction": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "prototype.patchAttributes": true,
          "findById": true,
          "finishCorrection": true,
          "startCorrection": true
        }
      }
    }
  },
  "content": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "create": true
        }
      }
    }
  },
  "notification": {
    "dataSource": "mongodb",
    "public": false
  },
  "feedback": {
    "dataSource": "mongodb",
    "public": false
  }
}
