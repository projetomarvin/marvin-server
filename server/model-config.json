{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongodb",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mongodb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongodb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": false
  },
  "course": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "findById": true,
          "create": true,
          "createNew": true,
          "prototype.__link__activities": true,
          "prototype.__unlink__activities": true,
          "prototype.__create__students": true,
          "prototype.__create__activities": true
        }
      }
    }
  },
  "student": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "login": true,
          "setPassword": true,
          "resetPassword": true,
          "findById": true,
          "getUsername": true,
          "pushToGit": true,
          "dryRun": true,
          "checkRepository": true,
          "prototype.patchAttributes": true,
          "showPending": true,
          "linkGithub": true,
          "linkGDrive": true,
          "prototype.__destroyById__notifications": true,
          "buy": true,
          "dice": true,
          "transferCoins": true,
          "prototype.__create__levelLogs": true
        }
      }
    }
  },
  "studentActivity": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "checkFiles": true,
          "checkFilesDrive": true,
          "finish": true,
          "excelFinish": true,
          "findById": true,
          "prototype.patchAttributes": true,
          "cancelCorrection": true,
          "answerCorrectionInvite": true,
          "fix": true
        }
      }
    }
  },
  "correction": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "prototype.patchAttributes": true,
          "findById": true,
          "finishCorrection": true,
          "finishExcel": true,
          "finishManual": true,
          "startCorrection": true,
          "dryRun": true,
          "prototype.__create__feedbacks": true,
          "owned": true
        }
      }
    }
  },
  "content": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "create": true
        }
      }
    }
  },
  "notification": {
    "dataSource": "mongodb",
    "public": false
  },
  "feedback": {
    "dataSource": "mongodb",
    "public": false
  },
  "levelLog": {
    "dataSource": "mongodb",
    "public": true
  },
  "pdf": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "findById": true,
          "create": true,
          "deleteById": true,
          "prototype.patchAttributes": true
        }
      }
    }
  },
  "exercise": {
    "dataSource": "mongodb",
    "public": false
  }
}
