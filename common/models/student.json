{
  "name": "student",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "activityNumber": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "correctionPoints": {
      "type": "number",
      "required": true,
      "default": 3
    },
    "coins": {
      "type": "number",
      "required": true,
      "default": 42
    },
    "githubAccessToken": {
      "type": "string"
    },
    "availableUntil": {
      "type": "any"
    },
    "panic": {
      "type": "boolean"
    },
    "pendingCorrection": {
      "type": "object"
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "course",
      "foreignKey": ""
    },
    "studentActivities": {
      "type": "hasMany",
      "model": "studentActivity",
      "foreignKey": ""
    },
    "correction": {
      "type": "belongsTo",
      "model": "correction",
      "foreignKey": ""
    },
    "notifications": {
      "type": "hasMany",
      "model": "notification",
      "foreignKey": ""
    },
    "levelLogs": {
      "type": "hasMany",
      "model": "levelLog",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": [
        "checkRepository",
        "__create__levelLogs",
        "__destroyById__notifications",
        "linkGithub",
        "pushToGit",
        "transferCoins",
        "buyCPoint"
      ]
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "getUsername"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "facilitator",
      "permission": "ALLOW",
      "property": "patchAttributes"
    }
  ],
  "methods": {}
}
