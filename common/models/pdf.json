{
  "name": "pdf",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "levelNumber": {
      "type": "number",
      "required": true
    },
    "levelName": {
      "type": "string",
      "required": true
    },
    "levelDescription": {
      "type": "string",
      "required": true
    },
    "specialInstructions": {
      "type": [
        "string"
      ]
    },
    "modifiedBy": {
      "type": "any"
    },
    "modifiedAt": {
      "type": "date"
    },
    "minGrade": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "trail": {
      "type": "string",
      "default": "main"
    }
  },
  "validations": [],
  "relations": {
    "exercises": {
      "type": "hasMany",
      "model": "exercise",
      "foreignKey": ""
    },
    "studentActivity": {
      "type": "hasMany",
      "model": "studentActivity",
      "foreignKey": ""
    },
    "courses": {
      "type": "hasAndBelongsToMany",
      "model": "course",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "facilitator",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "findById"
    }
  ],
  "methods": {}
}
