{
  "name": "activity",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": false
    },
    "PDFurl": {
      "type": "string",
      "required": true
    },
    "minGrade": {
      "type": "string",
      "required": true,
      "default": 0.7
    },
    "description": {
      "type": "string"
    },
    "exercises": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "course",
      "foreignKey": ""
    },
    "studentActivity": {
      "type": "hasMany",
      "model": "studentActivity",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
